framework: shell

/nfs:
    summary: Set up NFS dumping target
    require:
    - nfs-utils
    environment:
        REMOTE_TYPE: NFS

/nfs_early:
    summary: Set up NFS early dumping target
    require:
    - nfs-utils
    environment:
        REMOTE_TYPE: NFS_EARLY

/ssh:
    summary: Set up SSH dumping target
    environment:
        REMOTE_TYPE: SSH
